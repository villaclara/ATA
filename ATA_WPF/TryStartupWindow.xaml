<Window x:Class="ATA_WPF.TryStartupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ATA_WPF"
        mc:Ignorable="d"
        Title="TryStartupWindow" Height="100" Width="200"
        WindowStyle="None" AllowsTransparency="True" Background="Transparent" MouseDown="Window_MouseDown">
    <Border CornerRadius="2" BorderBrush="Black" BorderThickness="0" Padding="6" Background="#FFABCAF3">
        <Grid Background="#FFABCAF3">
            <TextBlock x:Name="TextBlock1" Text="" Grid.Column="0" Grid.Row="0" Margin="0,10,0,112" IsEnabled="False" />
            <TextBlock x:Name="ChekingTextBlock" Style="{StaticResource styleInfoTextBlock}" Text="Checking for update... " Grid.Row="0" TextAlignment="Center" Margin="2,40,0,0" FontFamily="Candara" />
        </Grid>
    </Border>
</Window>
