<Window x:Class="ATA_Updater.MainWindowUpdater"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ATA_Updater"
        mc:Ignorable="d"
        Title="MainWinwowUpdater" Height="100" Width="200"
        WindowStyle="None" AllowsTransparency="True" Background="Transparent" MouseDown="Window_MouseDown">

    <Window.Resources>
        <ResourceDictionary Source="DictionaryUpdater.xaml"/>
    </Window.Resources>

    <Border CornerRadius="2" BorderBrush="Black" BorderThickness="0" Padding="6" Background="#FFABCAF3">
        <Grid Background="#FFABCAF3">
            <TextBlock x:Name="TextBlock2" Text="" Grid.Column="0" Grid.Row="0" Margin="0,10,0,112" IsEnabled="False" />
            <TextBlock x:Name="CheckingTextBlock" Style="{StaticResource styleInfoTextBlock}" Text="Checking for update... " Grid.Row="0" TextAlignment="Center" FontFamily="Candara" Width="NaN" FontWeight="Bold" Foreground="White" Height="NaN" Margin="0,26,0,0" />
        </Grid>
    </Border>
</Window>
